{
	"name": "rose-pine-site",
	"version": "0.0.0",
	"license": "MIT",
	"type": "module",
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"preview": "svelte-kit preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"dependencies": {
		"@rose-pine/palette": "^2.1.0",
		"has-match": "^1.0.3",
		"svelte-i18n": "^3.3.13"
	},
	"devDependencies": {
		"@rgossiaux/svelte-headlessui": "^1.0.0-beta.12",
		"@sveltejs/adapter-netlify": "^1.0.0-next.51",
		"@sveltejs/kit": "^1.0.0-next.302",
		"autoprefixer": "^10.4.4",
		"postcss": "^8.4.12",
		"prettier": "^2.6.0",
		"prettier-plugin-tailwindcss": "^0.1.8",
		"svelte": "^3.46.4",
		"svelte-check": "^2.4.6",
		"svelte-preprocess": "^4.10.4",
		"tailwindcss": "^3.0.23",
		"tslib": "^2.3.1",
		"typescript": "^4.6.2"
	},
	"prettier": {
		"semi": false,
		"singleQuote": true,
		"useTabs": true,
		"plugins": [
			"prettier-plugin-tailwindcss"
		]
	}
}
